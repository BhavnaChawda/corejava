package Constracter;

public class OverlodingCons {
	int BorderWidth;
       String Color;
	public void setBorderWidth(int BorderWidth) {
		 this.BorderWidth = BorderWidth;
	}
	public int getBorderWidth() {
		return BorderWidth;
	}
	public void setColor(String Color) {
		 this.Color = Color;
	}
	public String getColor() {
		return Color;
	}
}
